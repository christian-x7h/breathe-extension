{
  "manifest_version": 3,
  "name": "Hello World",
  "version": "2.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "js": [
        "lib/jquery-3.6.1.min.js",
        "lib/psl.min.js",
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "views/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}